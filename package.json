{
  "name": "free-courses",
  "version": "1.0.0",
  "description": "free courses to developers",
  "main": "''",
  "scripts": {
    "tdd": "mocha src/**/*spec.js  --watch --require babel-register",
    "build": "webpack --config webpack.production.config.js --progress --profile --colors",
    "start": "webpack-dev-server --progress --profile --colors",
    "lint": "eslint --ext js --ext jsx src || exit 0",
    "dev": " webpack-dashboard -- webpack-dev-server",
    "test": "mocha src/**/*spec.js --require babel-register",
    "test:coverage": "nyc npm test",
    "readme": "node readme.js",
    "fix:lint": "eslint --fix --ext js --ext jsx src"
  },
  "nyc": {
    "reporter": [
      "text",
      "html"
    ]
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Leocardoso94/Free-Courses"
  },
  "author": "Leonardo Cardoso",
  "license": "MIT",
  "homepage": "https://freecourses.github.io/",
  "dependencies": {
    "autoprefixer": "^7.2.5",
    "copy-webpack-plugin": "^4.5.2",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "extract-text-webpack-plugin": "^3.0.0",
    "flag-icon-css": "^2.9.0",
    "font-awesome": "^4.7.0",
    "history": "^4.7.2",
    "html-webpack-plugin": "^2.26.0",
    "lodash.snakecase": "^4.1.1",
    "node-sass": "^4.3.0",
    "prop-types": "^15.6.0",
    "react": "^16.4.0",
    "react-addons-css-transition-group": "^15.6.2",
    "react-dom": "^16.4.0",
    "react-ga": "^2.3.5",
    "react-masonry-component": "^6.0.2",
    "react-reveal": "^0.7.3",
    "react-router": "^4.2.0",
    "react-router-dom": "^4.2.2",
    "sass-loader": "^6.0.2",
    "serviceworker-webpack-plugin": "^0.2.3",
    "social-tags-webpack-plugin": "^0.0.6",
    "sw-precache-webpack-plugin": "^0.11.4",
    "webpack": "^3.3.0",
    "webpack-cleanup-plugin": "^0.4.2",
    "webpack-dashboard": "^0.3.0",
    "webpack-dev-server": "^2.4.1",
    "webpack-pwa-manifest": "^3.4.1"
  },
  "devDependencies": {
    "babel-core": "^6.23.1",
    "babel-eslint": "^8.2.3",
    "babel-jest": "^20.0.3",
    "babel-loader": "^7.1",
    "babel-plugin-transform-async-to-generator": "^6.24.1",
    "babel-plugin-transform-class-properties": "^6.22.0",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-es2015": "6.22.0",
    "babel-preset-react": "^6.23.0",
    "babel-runtime": "^6.22.0",
    "bower": "^1.8.2",
    "chai": "^4.1.2",
    "css-loader": "0.26.1",
    "enzyme": "^2.9.1",
    "eslint": "^4.3.0",
    "eslint-plugin-jest": "^20.0.3",
    "eslint-plugin-react": "^7.5.1",
    "file-loader": "^0.10.0",
    "jest": "^20.0.4",
    "loglevel": "^1.4.1",
    "mocha": "^4.0.1",
    "nyc": "^11.2.1",
    "postcss-loader": "^1.2.2",
    "react-addons-test-utils": "^15.6.0",
    "react-hot-loader": "^3.0.0-beta.6",
    "react-test-renderer": "^15.6.1",
    "style-loader": "0.13.1",
    "url-loader": "0.5.7"
  },
  "jest": {
    "verbose": true,
    "bail": true,
    "collectCoverage": true,
    "coverageDirectory": "coverage",
    "moduleNameMapper": {
      "\\.(jpe?g|png|gif|eot|otf|webp|svg|ttf|woff2?|mp[34]|webm|wav|m4a|aac|oga)$": "<rootDir>/__mocks__/fileMock.js",
      "\\.(css|less|s[ac]ss|styl)$": "<rootDir>/__mocks__/styleMock.js",
      "app": "<rootDir>/src/app.jsx"
    },
    "moduleFileExtensions": [
      "js",
      "jsx"
    ],
    "moduleDirectories": [
      "node_modules"
    ]
  }
}
